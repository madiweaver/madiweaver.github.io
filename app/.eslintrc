{
	"plugins": [
		"react"
	],
	"settings": {
		"react": {
			"version": "16.6"
		}
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:import/errors"
	],
	"env": {
		"browser": true
	},
	"rules": {
		"react/forbid-prop-types": "off",
		"react/jsx-filename-extension": "off",
		"react/no-array-index-key": "off"
	}
}